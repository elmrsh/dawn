<script type="module">
  class ShopTheLook extends HTMLElement {
    constructor() {
      super();
      const padding = 20
      this.tabs = [...this.querySelectorAll('.stl-gallery__menu-item')]
      this.menu = this.querySelector('.stl-gallery__menu')
      this.nextButton = this.querySelector('.stl-next-button')
      let selected = this.tabs[0]
      this.menu.addEventListener('scroll', e => {
        this.tabs.forEach(tb => {
          const left = tb.offsetLeft - this.menu.scrollLeft
          if(left === padding) selected = tb
        })
      })
      this.nextButton.addEventListener('click', e => {
        if(!selected.nextElementSibling) return
        this.menu.scrollLeft = selected.nextElementSibling.offsetLeft - padding
      })

      this.mobileItems = this.querySelectorAll('.stl-gallery_item--mobile')

      this.mobileItems.forEach(itm => {
        itm.addEventListener('click', e => {
          console.log('click',e)
          if(!e.target.classList.contains('stl-gallery__detail')) return
          const active = itm.querySelector('.stl-gallery__detail.active')
          if(active) active.classList.remove('active')
          e.target.classList.add('active')
        })
      })

      this.menu.addEventListener("click", e => {
        console.log('Hicimos un click')
        const activeID = e.target.dataset.id
        const actives = this.querySelectorAll(`.active`)
        const matched = this.querySelectorAll(`[data-block="${activeID}"]`)
        actives.forEach(item => item.classList.remove('active'))
        matched.forEach(item => item.classList.add('active'))
        e.target.classList.add('active')
      })
    }
  }
  window.customElements.define('shop-the-look',ShopTheLook)
</script>

