<style>
  .stl-gallery {
    display: block;
    width: 100%;
    background: {{ section.settings.background }};
    padding: 10rem 2rem;
  }
  .stl-gallery__wrapper {
    display: grid;
    grid-template-columns: 30% 1fr;
    align-items: center;
  }
  .stl-gallery__title {
    padding: 10px;
    margin: 0;
  }
  .stl-gallery__image {
    display: block;
    width: 100%;
  }
  .stl-gallery__image img {
    display: block;
    width: 100%;
  }
  .stl-gallery__sidebar {
    width: fit-content;
  }
  .stl-gallery__menu {
    padding: 0;
    margin: 0;
    scroll-behavior: smooth;
  }
  .stl-gallery__menu-item {
    list-style: none;
    padding: .5em;
    width: fit-content;
    cursor: pointer;
    transition: all .5s;
    white-space: nowrap;
  }
  .stl-gallery__menu-item:hover {
    color: #000;
  }
  .stl-gallery__menu-item.active {
    background: {{ section.settings.highlight }};
    color: {{ section.settings.highlight-text }};
  }

  .stl-gallery_item {
    position: relative;
    opacity: 0;
    display: none;
    pointer-events: none;
    transition: all .5s;
    animation: itemFadeIn 1s;
  }
  .stl-gallery_item.active {
    opacity: 1;
    display: block;
    pointer-events: all;
  }

  .stl-gallery__detail {
    --detail-color: rgba(255,255,255,.7);
    display: block;
    background: var(--detail-color);
    backdrop-filter: blur(5px);
    padding: 10px;
    width: fit-content;
    max-width: 50%;
    position: absolute;
    top: var(--y-position);
    left:  var(--x-position);
    transform: scale(0);
    animation: detail .5s .5s forwards;
  }

  @keyframes detail {
    to {
      transform: scale(1);
    }
  }
  @keyframes itemFadeIn  {
    from {
      opacity: 0;
    }
  }

  .stl-gallery__detail--tail  {
    position: absolute;
    top: 100%;
    left: 0;
    width: 15px;
    height: 15px;
    line-height: 1;
    display: block;
  }
  .stl-gallery__detail--tail svg {
    width: 15px;
    height: 15px;
    fill: var(--detail-color);
    transform: translateY(-1px);
  }

  .stl-gallery_item--mobile, .stl-gallery_item--mobile.active {
    display: none;
  }
  .stl-next-button {
    display: none;
  }

  .stl-gallery__link {
    display: block;
    width: 100%;
    text-align: right;
    text-transform: uppercase;
    padding: 10px 0;
  }
  .stl-gallery__link:not(.active) {
    display: none;
  }
  .stl-gallery__link a {
    color: inherit;
  }
  .stl-gallery--product-link {
    color: inherit;
  }



  /* Tablets */
  @media (max-width: 749px) {
    .stl-gallery {
      padding:  10rem 0;
    }
    .stl-gallery__wrapper {
      margin: 0;
      max-width: 100%;
      padding: 0 0 0 10px;
    }
  }
  /* Mobile */
  @media (max-width: 640px) {
    .stl-gallery__wrapper {
      display: block;
      padding: 5rem 20px;
    }
    .stl-gallery__title {
      text-align: center;
      width: 100%;
    }
    .stl-gallery__sidebar {
      width: 100%;
      margin-bottom: 10px;
    }
    .stl-gallery__menu {
      display: grid;
      grid-auto-flow: column;
      gap: 5px;
      overflow-y: auto;
      overscroll-behavior-x: contain;
      scroll-snap-type: x mandatory;
      scrollbar-width: none;
    }
    .stl-gallery__menu::-webkit-scrollbar {
      display: none;
    }

    .stl-gallery__menu-item {
      font-size: 14px;
      scroll-snap-align: start;
    }

    /* Carrusel */
    .stl-navigation {
      display: grid;
      grid-template-columns: 1fr auto;
      max-width: 100%;
      gap: 10px;
    }
    .stl-navigation__items-wrapper {
      max-width: 100%;
      overflow-x: hidden;
    }

    /* Image */
    .stl-gallery_item--desktop, .stl-gallery_item--desktop.active {
      display: none;
    }

    .stl-gallery_item--mobile.active {
      display: block;
    }

    .stl-next-button {
      display: block;
      background: transparent;
      border: 0;
    }

    /* Detail  */
    .stl-gallery__detail:not(.active) > * {
      display: none;
    }
    .stl-gallery__detail {
      animation-delay: 0s;
    }
    .stl-gallery__detail:not(.active) {
      width: 40px;
      height: 40px;
      display: block;
      border-radius: 50%;
      background-color: rgba(0,0,0,.5);
      transform: scale(1) translate(100%,100%);
      box-shadow: 0 0 0 0 rgb(253, 251, 251);
      animation: pulse-white 3s infinite;
    }
    .stl-gallery__detail--tail {
      display: none;
    }
    .stl-gallery__detail:not(.active):before {
      content: "";
      display: block;
      width: 25px;
      height: 25px;
      background: rgba(255,255,255,.2);
      position: absolute;
      top: 50%;
      left: 50%;
      transform: translate(-50%,-50%);
      border-radius: 50%;
    }
    .stl-gallery__link {
      text-align: center;
    }
    @keyframes pulse-white {
      0% {
        box-shadow: 0 0 0 0 rgba(255, 255, 255, 0.7);
      }
      70% {
        box-shadow: 0 0 0 10px rgba(252, 252, 252, 0);
      }
      100% {
        box-shadow: 0 0 0 0 rgba(255, 82, 82, 0);
      }
    }
  }
</style>

